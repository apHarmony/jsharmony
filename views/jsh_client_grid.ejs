<div data-id="<%=model.id%>" class="xsearch xsearch_<%=model.class%>"></div>
<script class="xsearch_template_<%=model.class%>" type="text/x-ejs-template">
  <%% 
    var comparison_string = [];
    <%-instance%>.XExt.pushLOV(comparison_string,'contains','Contains');
    <%-instance%>.XExt.pushLOV(comparison_string,'notcontains','Does Not Contain');
    <%-instance%>.XExt.pushLOV(comparison_string,'=','Equals');
    <%-instance%>.XExt.pushLOV(comparison_string,'<>','Does Not Equal');
    <%-instance%>.XExt.pushLOV(comparison_string,'beginswith','Begins With');
    <%-instance%>.XExt.pushLOV(comparison_string,'endswith','Ends With');
    <%-instance%>.XExt.pushLOV(comparison_string,'null','Is Blank');
    <%-instance%>.XExt.pushLOV(comparison_string,'notnull','Is Not Blank');
    var comparison_numeric = [];
    <%-instance%>.XExt.pushLOV(comparison_numeric,'=','Equals');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'<>','Does Not Equal');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'>','>');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'>=','>=');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'<','<');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'<=','<=');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'null','Is Blank');
    <%-instance%>.XExt.pushLOV(comparison_numeric,'notnull','Is Not Blank');
    var comparison_date = comparison_numeric;
    var comparison_object = [];
    <%-instance%>.XExt.pushLOV(comparison_object,'=','Equals');
    <%-instance%>.XExt.pushLOV(comparison_object,'<>','Does Not Equal');
    <%-instance%>.XExt.pushLOV(comparison_object,'null','Is Blank');
    <%-instance%>.XExt.pushLOV(comparison_object,'notnull','Is Not Blank');
    var comparison_sound = function(comparison,search_sound){
      var rslt = comparison.slice(0);
      if(search_sound){ <%-instance%>.XExt.pushLOV(rslt,'soundslike','Sounds Like'); }
      return rslt;
    }
    %%>
  <%% for(var i=0;i<data.Items.length;i++){ var selected_column = null; %%>
    <div class="xsearch_expression xsearch_expression_<%%=i%%>">
      <a href="#" class="controls_wrapper" style="position: relative; display: inline-block;"><img src="/images/icon_list.png" onclick="<%-js("event.preventDefault(); event.stopPropagation(); XExt.ShowContextMenu('.search_context_"+model.class+"', this, event)" )%>"></a>
      Search Column: &nbsp;<select class="xsearch_column" onchange="<%-js('xmodel.controller.SearchColumnChange();')%>"><option value="ALL">Any...</option><%% for(var j=0;j<data.Fields.length;j++){ if(!data.Fields[j].caption || data.Fields[j].unbound) continue; %%><option value="<%%=data.Fields[j].name%%>" <%% if(data.Fields[j].name==data.Items[i].Column){ selected_column = data.Fields[j]; %%>selected="Selected"<%% } %%>><%%=data.Fields[j].caption%%></option><%% } %%></select>&nbsp; 
      <div class="xsearch_comparison_div" style="display:inline-block;width:140px;">

        <%% if(selected_column && (selected_column.comparison_type=='string')){ %%>
          <select class="xsearch_comparison" onchange="<%-js('xmodel.controller.SearchComparisonChange();')%>">
            <%%-xejs.renderLOV(comparison_sound(comparison_string,selected_column.search_sound),data.Items[i].Comparison)%%>
          </select>
        <%% } else if(selected_column && (selected_column.comparison_type=='numeric')){ %%>
          <select class="xsearch_comparison" onchange="<%-js('xmodel.controller.SearchComparisonChange();')%>">
            <%%-xejs.renderLOV(comparison_sound(comparison_numeric,selected_column.search_sound),data.Items[i].Comparison)%%>
          </select>
        <%% } else if(selected_column && (selected_column.comparison_type=='date')){ %%>
          <select class="xsearch_comparison" onchange="<%-js('xmodel.controller.SearchComparisonChange();')%>">
            <%%-xejs.renderLOV(comparison_sound(comparison_date,selected_column.search_sound),data.Items[i].Comparison)%%>
          </select>
        <%% } else if(selected_column && (selected_column.comparison_type=='object')){ %%>
          <select class="xsearch_comparison" onchange="<%-js('xmodel.controller.SearchComparisonChange();')%>">
            <%%-xejs.renderLOV(comparison_sound(comparison_object,selected_column.search_sound),data.Items[i].Comparison)%%>
          </select>
        <%% } else { %%>
          containing the value
          <select class="xsearch_comparison" onchange="<%-js('xmodel.controller.SearchComparisonChange();')%>" style="display:none;">
          </select>
        <%% } %%>
      </div>&nbsp; 
      <input type="text" class="xsearch_value <%%=xejs.iif((selected_column && (selected_column.comparison_type=='date')),'datepicker')%%>" value="<%%=xejs.iif((data.Items[i].Comparison=='null')||(data.Items[i].Comparison=='notnull'),'',data.Items[i].Value)%%>" onkeyup="<%-js('xmodel.controller.SearchKeyUp(event);')%>" style="<%%=xejs.iif((data.Items[i].Comparison=='null')||(data.Items[i].Comparison=='notnull'),'visibility:hidden;')%%>" />
        <%% if(i==(data.Items.length-1)){ %%>
        <a class="xsearch_apply" href="#" style="width:80px;" onclick="<%-js('xmodel.controller.RunSearch();')%>"><img src="/images/icon_search.png" alt="Search" title="Search" />GO</a>
        <%% } else { %%>
        <select name="xsearch_join" id="xsearch_join" class="xsearch_join" onchange="<%-js('xmodel.controller.SearchJoinChange(event);')%>" >
            <option value="and" <%% if(data.Items[i+1].Join == 'and' ) { %%>selected<%% } %%>>and</option>
            <option value="or" <%% if(data.Items[i+1].Join == 'or') { %%>selected<%% } %%>>or</option>
        </select>
        <%% } %%>
    </div>
    <%% } %%>
  </script>
    <div class="xcontext_menu search_context_<%=model.class%>">
        <a onclick="<%-js('jsh.$root(\'.xcontext_menu\').hide(); xmodel.controller.SearchAdd(jsh.xContentMenuItemData); return false;')%>"><img src="/images/icon_add.png" />Add</a>
        <a onclick="<%-js('jsh.$root(\'.xcontext_menu\').hide(); xmodel.controller.SearchRemove(jsh.xContentMenuItemData); return false;')%>"><img src="/images/icon_remove.png" />Remove</a>
    </div>
</div>
  <% if(!model.templates.grid){ %>
  <table class="xtbl xelem<%=model.class%> xform<%=model.class%>" data-id="<%=model.id%>" style="<%=model.tablestyle%>">
    <thead>
      <tr>
        <% if(model.inlinepopup){ %><th style="text-align:center;" nowrap>Select</th><% } %>
        <% function isHidden(f_name) {
            var c = model.display_layouts[model.display_layout].columns;
            return !(c.filter(function (it) {
                if (it.name == f_name) return true;
            }).length >0 );
        }%>
        <% for(var i=0;i<model.fields.length;i++){ var field = model.fields[i];
          if(!('caption' in field)) continue;
          if(('actions' in field)){
            if(!XExt.hasAction(field.actions,'BIU')) continue;
          }
          else if (!('html' in field)) continue; 
          var fieldclass = '';
          if(field.control && (field.control=='hidden')) fieldclass += ' tdhidden ';
          if(isHidden(field.name)) fieldclass += ' tdhidden';
            %>
        <th nowrap class="<%=field.sortclass%> thead<%=field.name%> <%=field.captionclass%> <%=fieldclass%>" style="<%=field.captionstyle%>"><% if(field.disable_sort||(field.type=='file')||field.unbound){ %><%=field.caption%><% } else { %><a href="#" onClick="<%-js('return xmodel.controller.Sort(this, \''+XExt.escapeHTML((field.lov && !field.lov.showcode)?('__'+jsh.uimap.codetxt+'__'+field.name):field.name)+'\');')%>"><%=field.caption%></a><% } %></th>
        <% } %>
        <%-xejs.iif(model.duplicate && model.commitlevel && (model.commitlevel != 'none') && XExt.hasAction(model.actions,'I'),'<th></th>')%>
        <%-xejs.iif(model.commitlevel && (model.commitlevel != 'none') && XExt.hasAction(model.actions,'D'),'<th></th>')%>
      </tr>
    </thead>
    <tbody class="xgrid_<%=model.class%>_placeholder"></tbody>
    <script class="xgrid_<%=model.class%>_template" type="text/x-ejs-template" data-target=".xgrid_<%=model.class%>_placeholder">
      <%% for(var j=0;j<datatable.length;j++){ var datarow=datatable[j]; var data = datarow; if((typeof startrowid == 'undefined') || (startrowid === null)) startrowid = j; var rowid = startrowid + j %%>
      <tr class="<%%=(rowid%2==1?'even':'odd')%%> <%-model.rowclass%> xrow_<%=model.class%> xrow" style="<%-model.rowstyle%>" data-id="<%%=rowid%%>">
        <% if(model.inlinepopup){ %><td align="center"><a href="#" onclick="<%-instance%>.XExt.popupSelect('<%=model.id%>',this); return false;">Select</a></td><% } %>
        <% for(var i=0;i<model.fields.length;i++){ var field = model.fields[i];
          if(!('caption' in field)) continue;
          if(('actions' in field)){
            if(!XExt.hasAction(field.actions,'BIU')) continue;
          }
          else if (!('html' in field)) continue;
          var format = 'undefined';
          if('format' in field) format = JSON.stringify(field.format);
          var fieldclass = '';
          if(field.control && (field.control=='hidden')) fieldclass += 'tdhidden';
          if(isHidden(field.name)) fieldclass += ' tdhidden';
          var maxlength = XExt.getMaxLength(field);
          %>
          <td style="<%=field.cellstyle%>" class="<%=field.cellclass%> <%=xejs.iif(field.control=='checkbox','xcheckbox_column')%> <%=xejs.iif(field.popuplov,'xpopuplov_column')%> <%=fieldclass%>"><% 
        if((field.control=='label') || (field.control=='html')){
          if(('link' in field) && (XExt.beginsWith(field.link,'#select'))){ %><a href="<%-field.link%>" onclick="<%-instance%>.XExt.popupSelect('<%=model.id%>',this);return false;" ><% }
          else if('link' in field){ %><a data-url="<%-field.link%>" href="<%-XExt.replaceTempEJSTags(field.link)%>" class="xform_link" onclick="<%-js(field.link_onclick)%>"><% }
          if((field.control=='html') && ('value' in field)){
            %><%-XExt.replaceTempEJSTags(field.value)%><%
          } else if(field.control=='html'){
            %><div data-id="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.controlclass%> <%=field.name%> xform_ctrl xelem<%=model.class%> xform_html" onclick="return (function(){<%-jslocals%><%-field.onclick%>}).call(this);"></div><%
          } else if(field.control=='label'){
            %><div data-id="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.controlclass%> <%=field.name%> xform_ctrl xelem<%=model.class%> xform_label <%=xejs.iif(field.value,'xform_label_static')%>" onclick="return (function(){<%-jslocals%><%-field.onclick%>}).call(this);"><%
            if(field.value && (field.value.indexOf('<#')>=0)){  }
            else if(field.value){ %><%-field.value%><% }
            else { %><%-xejs.GetValue(field, model)%><% }
            %></div><%
          }
          if('link' in field){ %></a><% }
        }
        else if(field.control=='textbox'){ %><input type="<%=xejs.getInputType(field)%>" data-id="<%=field.name%>" value="<%-xejs.GetValue(field, model)%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> <%=field.controlclass%>" <%-xejs.iif(maxlength >= 0,'maxlength="'+maxlength+'"')%> /><% }
        else if(field.control=='textzoom'){ %><div class="xform_ctrl_textzoom_container"><textarea data-id="<%=field.name%>" name="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xform_ctrl_textzoom xelem<%=model.class%> <%=field.controlclass%>"><%-xejs.GetValue(field, model)%></textarea><a href="#" class="xform_ctrl xtextzoom icon" onclick="var jctrl = <%-instance%>.$(this).prev('.<%=field.name%>'); <%-instance%>.XExt.ZoomEdit(jctrl.val(),'<%=field.caption_ext||field.caption%>',{readonly:!jctrl.hasClass('editable')},function(rslt){ <%-instance%>.XExt.setFormControl(<%-instance%>.XExt.getForm('<%=model.id%>'),'<%=field.name%>',rslt); jctrl.focus(); }); return false;"><img src="/images/icon_search.png" height="14" /></a></div><% }
        else if(field.control=='password'){ %><input type="password" data-id="<%=field.name%>" value="<%-xejs.GetValue(field, model)%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> <%=field.controlclass%>" <%-xejs.iif(maxlength >= 0,'maxlength="'+maxlength+'"')%> /><% }
        else if(field.control=='date'){ %><input data-id="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> datepicker <%=field.controlclass%>" value="<%-xejs.GetValue(field, model)%>" maxlength="10" /><a href="#" onclick="return false;" class="xform_ctrl xelem<%=model.class%> datepicker_handle nofocus" tabindex="-1"><img src="/images/spacer.gif" /></a><% }
        else if(field.control=='textarea'){ %><textarea data-id="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> <%=field.controlclass%>"><%-xejs.GetValue(field, model)%></textarea><% }
        else if(field.control=='dropdown'){ %><select data-id="<%=field.name%>" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> dropdown <%=field.controlclass%>"><option><%-xejs.GetValue(field, model)%></option></select><% }
        else if(field.control=='hidden'){ %><input type="hidden" data-id="<%=field.name%>" value="<%-xejs.GetValue(field, model)%>" class="<%=field.name%> xform_ctrl xelem<%=model.class%> <%=field.controlclass%>" /><% }
        else if(field.control=='checkbox'){ %><div class="xform_ctrl xform_checkbox_container xelem<%=model.class%>"><input data-id="<%=field.name%>" type="checkbox" style="<%=field.controlstyle%>" class="<%=field.name%> xform_ctrl checkbox xelem<%=model.class%> <%=field.controlclass%>" /></div><% }
        else if(field.control=='button'){ 
          if('link' in field){ %><input data-id="<%=field.name%>" type="button" class="xform_ctrl xform_ctrl_button xelem<%=model.class%> <%=field.name%> <%=field.controlclass%>" style="<%=field.controlstyle%>" value="<%-xejs.escapeHTMLN(field.value)%>" data-url="<%-field.link%>" onclick="<%-js(field.onclick)%>" /><% }
          else{ %><input data-id="<%=field.name%>" type="button" data-model="<%=model.id%>" class="xform_ctrl xform_ctrl_button xelem<%=model.class%> <%=field.name%> <%=field.controlclass%>" style="<%=field.controlstyle%>" value="<%-xejs.escapeHTMLN(field.value)%>" onclick="<%-js(field.onclick)%>" /><% }
        }
        else if(field.control=='linkbutton'){ 
          if('link' in field){ %><a data-id="<%=field.name%>" href="#" class="xform_ctrl xelem<%=model.class%> <%=field.name%> <%=field.controlclass%> xform_label <%=xejs.iif(field.value,'xform_label_static')%>" style="<%=field.controlstyle%>" data-url="<%-field.link%>" onclick="<%-js(field.onclick)%>"><%=xejs.ifnull(field.value,'')%></a><% }
          else { %><a data-id="<%=field.name%>" data-model="<%=model.id%>" href="#" style="<%=field.controlstyle%>" class="xform_ctrl xelem<%=model.class%> <%=field.name%> <%=field.controlclass%> xform_label <%=xejs.iif(field.value,'xform_label_static')%>" onclick="<%-js(field.onclick)%>"><%=xejs.ifnull(field.value,'')%></a><% }
        }
        else if(field.control=='file_download'){ %>
            <div data-id="<%=field.name%>" class="<%=field.name%> xform_file_download xelem<%=model.class%> <%=field.controlclass%>" style="<%=field.controlstyle%>">
              <% if(field.controlparams.download_button){ %><a class="<%=field.name%>_download linkbutton xform_file_download_button" href="#" onclick="<%-js('xmodel.controller.DownloadFile(\''+XExt.escapeHTML(field.name)+'\',\''+XExt.escapeHTML(field.caption_ext||field.caption)+'\','+XExt.ejsDelimiter.open+'=rowid'+XExt.ejsDelimiter.close+');')%>"><img src="/images/icon_download.png" /><%=field.controlparams.download_button%></a><% } %>
              <input type="hidden" data-id="<%=field.name%>_dbexists" class="<%=field.name%>_dbexists xform_file_dbexists xform_ctrl xelem<%=model.class%> <%=field.controlclass%>" value="<%-xejs.GetValue(field, model)%>" />
            </div>
        <% }
        else if(field.control=='image'){ %>
            <% if(field.controlparams.preview_on_click){ %><a class="<%=field.name%>_preview xform_image_preview xelem<%=model.class%>" href="#" onclick="<%-js('xmodel.controller.FilePreview(\''+XExt.escapeHTML(field.name)+'\',\''+XExt.escapeHTML(field.caption_ext||field.caption)+'\','+XExt.ejsDelimiter.open+'=rowid'+XExt.ejsDelimiter.close+');')%>"><% } %>
            <img data-id="<%=field.name%>" class="<%=field.name%> xform_image xelem<%=model.class%> <%=field.name%>_thumbnail xform_file_thumbnail <%=field.controlclass%>" style="<%=field.controlstyle%>" />
            <% if(field.controlparams.preview_on_click){ %></a><% } %>
            <input type="hidden" data-id="<%=field.name%>_dbexists" class="<%=field.name%>_dbexists xform_file_dbexists xform_ctrl xelem<%=model.class%> <%=field.controlclass%>" value="<%-xejs.GetValue(field, model)%>" />
        <% }
        //Add a hidden field for field value if it is both static and updatable
        if(XExt.hasAction(field.actions,'BIU') && _.includes(['html','label','linkbutton','button'],field.control)){
          %><input data-id="<%=field.name%>_field_value" type="hidden" class="<%=field.name%>_field_value xform_ctrl xelem<%=model.class%>" value="<%-xejs.GetValue(field, model)%>" /><%
        }
        if(field.popuplov){ %><a href="#" data-id="<%=field.name%>_xlookup" class="xform_ctrl xlookup icon <%=field.name%>_xlookup" data-codeval="<%=field.controlparams.codeval%>" data-model="<%=model.id%>" onclick="<%-instance%>.XExt.popupShow('<%=field.model.id%>','<%=field.name%>','<%=field.caption_ext||field.caption%> Selection',<%-instance%>.$(this).prev('.<%=field.name%>'),this,{rowid: <%%=rowid%%>,OnControlUpdate:function(obj,e){ <%-instance%>.XModels['<%=model.id%>'].controller.editablegrid.ControlUpdate(obj,e); }})"><img src="/images/icon_search.png" height="14" style="position:relative;top:-1px;" /></a><% }
          %></td>
        <% } %>
        <% if(model.duplicate && model.commitlevel && (model.commitlevel != 'none') && XExt.hasAction(model.actions,'I')){ %><td align="center"><a href="#" class="xgrid_action" data-model="<%=model.id%>" data-actions="I" class="row_independent" onclick="<%-js('xmodel.controller.DuplicateRow(\''+XExt.escapeHTML(model.duplicate.model.id)+'\','+XExt.ejsDelimiter.open+'=rowid'+XExt.ejsDelimiter.close+', this, \''+XExt.escapeHTML((model.duplicate.bindings?JSON.stringify(model.duplicate.bindings):'{}'))+'\');')%>" style="<%-xejs.visible(XExt.hasAction(model.actions,'I'))%>"><img src="/images/icon_copy.png" height="14" alt="Duplicate" title="Duplicate" /></a></td><%}%>
        <% if(model.commitlevel && (model.commitlevel != 'none') && XExt.hasAction(model.actions,'D')){%><td align="center"><a href="#" class="xgrid_action" onclick="<%-js('xmodel.controller.DeleteRow('+XExt.ejsDelimiter.open+'=rowid'+XExt.ejsDelimiter.close+');')%>" style="<%-xejs.visible(XExt.hasAction(model.actions,'D'))%>"><img src="/images/icon_delete2.png" height="14" alt="Delete" title="Delete" /></a></td><%}%>
      </tr>
      <%% } %%>
    </script>
